{% extends 'user.html.twig' %}

{% block title %} {{ namepage }} {% endblock %}

    {% block dashboard %}
        <a href="{{ path('user') }}" class="list-group-item waves-effect active">
            <i class="fas fa-chart-pie mr-3"></i>{{ namepage }}
        </a>
    {% endblock %}

    {% block namepage %}
        <span>{{ namepage }}</span>
    {% endblock %}

{% block body %}
        <!--Grid column-->
        <div class="col-md-6 mb-4 mx-auto w-75">

            <!--Card-->
            <div class="card">

                <!--Card content-->
                <div class="card-body">
                    <h2 align="center">My Last Rated Event</h2>
                    <!-- Table  -->
                    <table class="table table-hover">
                        <!-- Table head -->
                        <thead class="blue-grey lighten-4">
                        <tr>
                            <th>#</th>
                            <th>Title</th>
                            <th>Date Start</th>
                            <th>Date End</th>
                            <th>Rate</th>
                        </tr>
                        </thead>
                        <!-- Table head -->

                        <!-- Table body -->
                        <tbody>
                            <tr>
                                <th scope="row">1</th>
                                <td> Cell 1 </td>
                                <td> Cell 2</td>
                                <td> Cell 3 </td>
                                <td> Cell 3 </td>
                            </tr>
                            <tr>
                                <th scope="row">2</th>
                                <td> Cell 1 </td>
                                <td> Cell 2</td>
                                <td> Cell 3 </td>
                                <td> Cell 3 </td>
                            </tr>
                            <tr>
                                <th scope="row">3</th>
                                <td> Cell 1 </td>
                                <td> Cell 2</td>
                                <td> Cell 3 </td>
                                <td> Cell 3 </td>
                            </tr>
                        </tbody>
                        <!-- Table body -->
                    </table>
                    <!-- Table  -->

                </div>

            </div>
            <!--/.Card-->

        </div>
        <!--Grid column-->

{% endblock %}
